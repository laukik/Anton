    <div class="row">
      <div class="col-lg-4">
        <h1><span class="badge badge-default form form-control form-input">Add New User </span></h1>
        <form class="form" action="/user" method="post">
          <input class="form form-control form-input" style="margin-bottom:5px" type="text" required="true" placeholder="User Name" name="username" >
          <input class="form form-control form-input" style="margin-bottom:5px" type="text" required="true" placeholder="NTNET" name="ntnet" >
          <input class="form form-control form-input"  type="text" style="margin-bottom:5px" required="true"   placeholder="Employee ID" name="employeeId" >
          <!-- <input class="form form-control form-input" style="margin-bottom:5px"  type="text" required="true" placeholder="Application" name="application" > -->


          <select name = "application" placeholder="Application" class="combobox form form-control" style="margin-bottom:5px" >
            <% for( var i = 0; i < app.length;i++){ %>
              <option value="<%= app[i].app %>" ><%= app[i].app %></option>
              <% } %>
          </select>


          <button type="submit" style="margin-top:8px" class="form form-control btn btn-success">Submit</button>
        </form>
      </div>
      <div class="col-lg-8">
        <h1><span class="badge badge-default form form-control form-input">Existing Users </span></h1>
        <table class="table table-striped table-bordered table-hover">
          <thead>
          </tr>
            <tr>
              <th>#</th>
              <th> UserName</th>
              <th> NTNET</th>
              <th> Employee ID</th>
              <th> Application</th>
          </thead>
          <tbody>
              <% for( var i = 0 ; i < users.length; i ++){ %>
                <!-- <span class="badge badge-primary"> -->
                <tr>
                  <th scope="row"><%= i %></th>
                  <td><%= users[i].username %></td>
                  <td><%= users[i].ntnet %></td>
                  <td><%= users[i].employeeId %></td>
                  <td><%= users[i].application %></td>
                </tr>
              <% } %>
          </tbody>
        </table>
      </div>

    </div>
  </body>
